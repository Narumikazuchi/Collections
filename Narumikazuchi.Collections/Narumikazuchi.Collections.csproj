<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<TreatWarningsAsErrors>True</TreatWarningsAsErrors>
		<WarningsNotAsErrors>1591,0282</WarningsNotAsErrors>
		<NoWarn>1701;1702,0282</NoWarn>
		<PlatformTarget>x64</PlatformTarget>
		<DocumentationFile>Narumikazuchi.Collections.xml</DocumentationFile>
		<RepositoryUrl>https://github.com/Narumikazuchi/Collections</RepositoryUrl>
		<Authors>Narumikazuchi</Authors>
		<Copyright>Â©2021 Narumikazuchi</Copyright>
		<Description>Provides some extensions methods for collections as well as introducing new collections designed for big datasets by using optimized enumeration therefor improving speed of foreach-loops significantly. For more information visit the [wiki](https://github.com/Narumikazuchi/Collections/wiki).</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageReleaseNotes></PackageReleaseNotes>
		<Version>6.0.0</Version>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageProjectUrl>https://github.com/Narumikazuchi/Collections/wiki</PackageProjectUrl>
		<PackageIcon>logo.png</PackageIcon>
		<PackageReadmeFile>README nuget.md</PackageReadmeFile>
		<PackageTags>Collections; Observable; Trie; Binary Tree; Big Datasets; Enumeration</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\logo.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include="..\README nuget.md">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Narumikazuchi" Version="2.4.0" />
	</ItemGroup>

</Project>
